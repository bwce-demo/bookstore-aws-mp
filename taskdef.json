{
    "family": "bwce-bookstore-taskdef",
    "containerDefinitions": [
        {
            "image": "928133119666.dkr.ecr.us-east-1.amazonaws.com/bwce-bookstore:latest",
            "name": "bwce-bookstore",
            "cpu": 10,
            "memory": 1024,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ],
            "environment": [
                {
                    "name": "BW_LOGLEVEL",
                    "value": "DEBUG"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "bwce-bookstore-log",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "bwce"
                }
	    }
        }
    ]
}
